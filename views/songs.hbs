<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Listen</th>
        </tr>
    </thead>
    <tbody>
        {{#each results}}
        <tr>
            <td>{{ this.name }}</td>
            <td><audio controls alt=“audio” src=“{{this.preview_url}}” type=“audio/mpeg”></audio></td>
        </tr>
        {{/each}}
    </tbody>
</table>